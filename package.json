{
  "name": "@kirmas/cv",
  "version": "1.2.0",
  "description": "Andrii Kirmas - Senior Software Engineer",
  "homepage": "https://askirmas.github.io/cv/",
  "scripts": {
    "build": "npm run test",
    "postbuild": "npm run compile/pdf",
    "test": "npm run compile/html && npm run compile/sass",
    "compile/md": "npm run md --silent > ./Andrii_Kirmas.md",
    "compile/html": "ts-node --project page/tsconfig.json page/html.ts",
    "postcompile/html": "./absolutize.sh",
    "md": "ts-node cli/md.ts",
    "md@dev": "nodemon",
    "sass@dev": "npm run sass -- --watch",
    "compile/sass": "npm run sass",
    "sass": "sass --no-indented --no-source-map --style=compressed --load-path=node_modules styles:.",
    "compile/pdf": "node cli/pdf.js",
    "postinstall": "npm run --silent postinstall:ubuntu-1 || npm run --silent postinstall:mac || exit 0",
    "postinstall:ubuntu-1": "nohup xdg-open index.html >/dev/null 2>&1 &",
    "postinstall:ubuntu-2": "nohup x-www-browser index.html >/dev/null 2>&1 &", 
    "postinstall:mac": "open index.html"
  },
  "keywords": [
    "Andrii Kirmas",
    "Senior Software Engineer",
    "CV"
  ],
  "author": {
    "name": "Andrii Kirmas",
    "email": "kirmas@phystech.edu",
    "url": "https://github.com/askirmas",
    "linkedIn": "https://www.linkedin.com/in/kirmas/",
    "github": "https://github.com/askirmas",
    "phex": "e2701c48e2"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/puppeteer": "^3.0.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "git-hooks-wrapper": "^0.7.1",
    "jest": "^26.2.2",
    "material-components-web": "^7.0.0",
    "nodemon": "^2.0.4",
    "np": "^6.4.0",
    "puppeteer": "^5.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "np": {
    "yarn": false
  },
  "files": [
    "index.html",
    "Andrii_Kirmas.html",
    "Andrii_Kirmas.pdf",
    "Andrii_Kirmas.css",
    "style.css"
  ]
}
