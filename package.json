{
  "name": "@kirmas/cv",
  "version": "1.0.0",
  "description": "Andrii Kirmas CV",
  "homepage": "https://askirmas.github.io/cv/",
  "scripts": {
    "build": "npm run test",
    "postbuild": "npm run compile/pdf",
    "test": "npm run compile/html && npm run compile/sass",
    "compile/md": "npm run md --silent > ./Andrii_Kirmas.md",
    "compile/html": "./absolutize.sh",
    "md": "ts-node cli/md.ts",
    "md@dev": "nodemon",
    "sass@dev": "npm run sass -- --watch",
    "compile/sass": "npm run sass",
    "sass": "sass --no-indented --no-source-map --style=compressed --load-path=node_modules styles:.",
    "compile/pdf": "node cli/pdf.js"
  },
  "keywords": [
    "Andrii Kirmas",
    "CV"
  ],
  "author": {
    "name": "Andrii Kirmas",
    "email": "kirmas@phystech.edu",
    "url": "https://www.linkedin.com/in/kirmas/"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/puppeteer": "^3.0.1",
    "jest": "^26.2.2",
    "material-components-web": "^7.0.0",
    "nodemon": "^2.0.4",
    "np": "^6.4.0",
    "puppeteer": "^5.2.1",
    "sass": "^1.26.10",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "git-hooks-wrapper": "^0.7.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "np": {
    "yarn": false
  },
  "files": [
    "index.html",
    "Andrii_Kirmas.html",
    "Andrii_Kirmas.pdf",
    "Andrii_Kirmas.css",
    "style.css"
  ]
}
