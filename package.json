{
  "name": "@kirmas/cv",
  "version": "1.4.0",
  "description": "Andrii Kirmas - Senior Full Stack Software Engineer",
  "homepage": "https://askirmas.github.io/cv/",
  "main": "index.js",
  "bin": {
    "kirmas-cv": "index.js",
    "andrii_kirmas-cv": "index.js"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "npm run sass@dev & npm run html@dev",
    "build": "npm run test --pdf_gen=1",
    "pretest": "npm run compile/html && npm run compile/sass",
    "test": "npm run spec",
    "test@ci": "npm run test --blur=2 --threshold=0.01",
    "-compile/md": "npm run md --silent > ./Andrii_Kirmas.md",
    "compile/html": "ts-node --project page/tsconfig.json page/html.ts",
    "postcompile/html": "./absolutize.sh",
    "html@dev": "nodemon",
    "-md": "ts-node cli/md.ts",
    "-md@dev": "nodemon",
    "sass@dev": "npm run sass -- --watch",
    "compile/sass": "npm run sass",
    "sass": "sass --no-indented --no-source-map --style=compressed --load-path=node_modules styles:.",
    "spec": "jest --config jest.config.json",
    "postinstall": "node index.js",
    "conf_var": "echo \"<$npm_config_registry>\""
  },
  "keywords": [
    "Andrii Kirmas",
    "Senior",
    "Full Stack",
    "Software",
    "Engineer",
    "CV"
  ],
  "author": {
    "name": "Andrii Kirmas",
    "email": "kirmas@phystech.edu",
    "url": "https://github.com/askirmas",
    "linkedIn": "https://www.linkedin.com/in/kirmas/",
    "github": "https://github.com/askirmas",
    "phex": "e2701c48e2"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/jest-image-snapshot": "^3.1.0",
    "@types/puppeteer": "^3.0.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "git-hooks-wrapper": "^0.7.1",
    "jest": "^26.3.0",
    "jest-image-snapshot": "^4.1.0",
    "material-components-web": "^7.0.0",
    "nodemon": "^2.0.4",
    "np": "^6.4.0",
    "puppeteer": "^5.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {},
  "publishConfig": {
    "access": "public"
  },
